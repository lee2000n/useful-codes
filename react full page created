</div>

</p>




)




}


}

class Quicklink extends React.Component
{


componentDidMount()
{ if(window.location.href.indexOf("pp5") > -1) {
       document.querySelector("#pp5").focus();
document.querySelector("#pp5").click();
 
}
 if(window.location.href.indexOf("s1") > -1) {
       document.querySelector("#s1").focus();
document.querySelector("#s1").click();
 
}
 if(window.location.href.indexOf("c1") > -1) {
       document.querySelector("#c1").focus();
document.querySelector("#c1").click();
 
}
 if(window.location.href.indexOf("m1") > -1) {
       document.querySelector("#m1").focus();
document.querySelector("#m1").click();



    }

 if(window.location.href.indexOf("pp7") > -1) {
       document.querySelector("#pp7").focus();
document.querySelector("#pp7").click();



    }


}
showfocus()
{ 
 
if( document.querySelector("#pp5"))
{
window.location.href="https://www.mystudentplan.ca/redriver-international/en/homepage#/#pp5";
       document.querySelector("#pp5").focus();

 }

    }

showcg()
{ 
 
if( document.querySelector("#c1"))
{
window.location.href="https://www.mystudentplan.ca/redriver-international/en/homepage#/#c1";
       document.querySelector("#c1").focus();

 }

    }

showps()
{ 
 
if( document.querySelector("#s1"))
{
window.location.href="https://www.mystudentplan.ca/redriver-international/en/homepage#/#s1";
       document.querySelector("#s1").focus();

 }

    }



render()
{
var img1="/_images/quicklinks/schedule.png";
var img2="/_images/quicklinks/benefits-card.png";
var img3="/_images/quicklinks/prescription.png";
var img4="/_images/quicklinks/links.png";




