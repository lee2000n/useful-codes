class Coverages extends React.Component
{





constructor() {
 document.querySelector('#mod6286d02d_214e_4434_8718_c0a268984d8b').style.display= "none";
  document.querySelector('#mod068bc57b_c607_4837_88e5_2f587afb495d').style.display= "none";
    super();
    this.state = {
coverinfo :true,
   intlinfo:true,
einfo: true,
familyinfo: true,
