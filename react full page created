    // You can use redux for this.
    this.setState({
chrio :false,
   den :true ,
massage: true,

    });
 }



  showden() {
    // You can use redux for this.
    this.setState({
chrio :true,
   den :false,
massage: true,

    });
 
  }

 showmass() {
    // You can use redux for this.
    this.setState({
chrio :true,
   den :true,
massage: false,

    });
 
  }
 






