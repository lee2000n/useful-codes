highlightSearch() {
 






     var text = document.getElementById("searchtt").value;
 var query = new RegExp("(\\b" + text + "\\b)", "gim");

if(text.includes("exclusion"))
{
document.getElementById("m7").style.color="red";

this.setState({
result: true,
haveresult: false
    });


}
else
{
document.getElementById("m7").style.color="black";

this.setState({
result: false,
haveresult: true
    });




if(text.match(/\S/)  && !text.includes("iframe")&& !text.includes("href") && text.length>3 )
{
   var count=0;
var ids="Progress";
var buttonids="m";
for (var i=1;i<14; i++)


{
  if(document.getElementById(ids+i).innerText.match(query))
  
{
    var enew = document.getElementById(ids+i).innerHTML.replace(/(<span>|<\/span>)/igm, "");

   document.getElementById(ids+i).innerHTML = enew;

var newe =  enew .replace(query, "<span>$1</span>");
 document.getElementById(ids+i).innerHTML =newe;




}


else
{

 var enew1 = document.getElementById(ids+i).innerHTML.replace(query, "<span>$1</span>");

   document.getElementById(ids+i).innerHTML = enew1;

var newe1 =  enew1.replace(/(<span>|<\/span>)/igm, "");
 document.getElementById(ids+i).innerHTML =newe1;
}


}

if(document.getElementById("Progress1").innerText.match(query))

{count++;
document.getElementById("m1").style.color="red";





}

else
{
document.getElementById("m1").style.color="black";

}



if(document.getElementById("Progress2").innerText.match(query)||document.getElementById("Progress3").innerText.match(query))

{count++;
document.getElementById("m2").style.color="red";

}

else
{
document.getElementById("m2").style.color="black";

}


if(document.getElementById("Progress4").innerText.match(query))

{count++;
document.getElementById("m3").style.color="red";

}
else
{
document.getElementById("m3").style.color="black";

}

if(document.getElementById("Progress5").innerText.match(query))

{count++;
document.getElementById("m4").style.color="red";


}
else
{
document.getElementById("m4").style.color="black";

}

if(document.getElementById("Progress6").innerText.match(query)||document.getElementById("Progress7").innerText.match(query)||document.getElementById("Progress8").innerText.match(query)||document.getElementById("Progress9").innerText.match(query)||document.getElementById("Progress10").innerText.match(query)||document.getElementById("Progress11").innerText.match(query)||document.getElementById("Progress12").innerText.match(query))

{count++;
document.getElementById("m5").style.color="red";


}

else
{
document.getElementById("m5").style.color="black";

}
if(document.getElementById("Progress13").innerText.match(query))

{count++;
document.getElementById("m6").style.color="red";


}

else
{
document.getElementById("m6").style.color="black";

}
if(count==0)
{
this.setState({
result:false,
haveresult: true
    });


}
else if(count==document.getElementById("Progress6").length+document.getElementById("Progress5").length+document.getElementById("Progress4").length+document.getElementById("Progress3").length+document.getElementById("Progress2").length+document.getElementById("Progress1").length+document.getElementById("Progress7").length+document.getElementById("Progress8").length+document.getElementById("Progress9").length+document.getElementById("Progress10").length+document.getElementById("Progress11").length+document.getElementById("Progress12").length+document.getElementById("Progress13").length )
{
this.setState({
result:true,
haveresult: false,
    });

}




else{
     this.setState({
result: true,
haveresult: false
    });
       }



}

else
{alert("please enter something more specific to search");
document.getElementById("m6").style.color="black";
document.getElementById("m5").style.color="black";
document.getElementById("m4").style.color="black";
document.getElementById("m3").style.color="black";
document.getElementById("m2").style.color="black";
document.getElementById("m1").style.color="black";

 this.setState({
result: true,
haveresult: true
    });
var ids1="Progress";







for (var i=1;i<14; i++)


{
   
var enew1 = document.getElementById(ids1+i).innerHTML.replace(query, "<span>$1</span>");

   document.getElementById(ids1+i).innerHTML = enew1;

var newe1 =  enew1.replace(/(<span>|<\/span>)/igm, "");
 document.getElementById(ids1+i).innerHTML =newe1;
}





}

}


}

 keyPress(event) {
    if(event.charCode == 13) {
      event.preventDefault();
    }
  }


  render() {

    return (

      <div className="buttonposition" >
<div id="searches">
           <input type="text" id="searchtt" placeholder="Search..." onKeyPress={this.keyPress.bind(this)} />
<img id="startBtn " style={{background: "#bdbdbd", width:"20px",cursor:"pointer",marginTop:"-1px"}} src="https://www.mystudentplan.ca/_images/icon_search.png "   onClick={this.highlightSearch}/></div>
<span className={this.state.result?"all":"showresult"} style={{color: "red"}}>{this.state.result? "  please click the red buttons below for details.": "  No result found ? please check on other pages "}</span>
<span className={this.state.haveresult?"all":"showresult"} style={{color: "#529396"}}>{this.state.haveresult? "  ": " Please click the red buttons below for details."}</span>
 <br/>

        <input type="button" className="btn btn-default" id="m1" value="myBenefits at a Glance" name="myBenefits at a Glance" onClick={this.showshowglance} />
        <input type="button" value="International Insurance" className="btn btn-default" id="m2" name="International Insurance" onClick={this.showshowint } />
        <input type="button" value="Repatriation" className="btn btn-default" name="Repatriation"  id="m3" onClick={this.showshowrepates} />
        <input type="button" value="Prescription Drug" className="btn btn-default"  id="m4" onClick={this.showprescription} />
       <input type="button" value="Dental" className="btn btn-default"  id="m5" onClick={this.showdental} />
       <input type="button" value="PHRA" className="btn btn-default"  id="m6" onClick={this.showpersonal} />
       <input type="button" value="Exclusion" className="btn btn-default"  id="m7" onClick={this.showexclusion} />
<br/><br/>
<Showint aaa={this.state.intel} />
<Showglance aa={this.state.glance} />
<Showrepates rep={this.state.reshow} />
<Prescription pres={this.state.pres} />
<Dental den={this.state.dental} />
<Personal vid={this.state.personal} />
<Exclusion exc={this.state.exclusion} />


<Quicklink />


      </div>

    );
  }

}

class Showglance extends React.Component {



  render() {




    return (
<p className={this.props.aa? 'all' : 'allshow'} >

<p className="card mb-4 box-shadow"><p className="card-header"><h3>myBenefits at a Glance</h3></p><p className="card-body"  id="Progress1">
mybenefits at a glance provides general information. Coverage for eligible costs is based on the contract detail. Select the benefit for additional coverage details.<br/><br/>

<strong>Vision: </strong><br/>
Reimbursed at 100% of the reasonable and customary charges.
<br />
Limited to one trip per benefit year.
<br />
<br />
<strong>Vision:</strong><br />
Reimbursed at 100% to maximum of $80 every 24 months for eye exam, and 100% to a maximum of $200 for glasses or contact lenses.<br />
<br />
<strong>Prescription Drugs:</strong><br />
Reimbursed at 85% to a maximum of $3,000 per benefit year.<br />
Based on the NASA Formulary with a generic rider.
<br />
<br />
<strong>Health Practitioners:</strong><br />
Covered practitioners include: Acupuncturist, podiatrist/chiropodist, chiropractor, massage therapist, naturopath, dietitian, osteopath, physiotherapist, psychologist or registered social worker, and speech therapist. Practitioners must be registered and licensed in their field of practice. For detailing&rsquo;s outlining amounts covered please visit supplemental health.<br />
<br />
<strong>Medical Equipment & Supplies:</strong><br />
Overall maximum of $2000 per benefit year. A physician's prescription is required. Pre-authorization is suggested. <br />
<br />
<strong>Dental Coverage:</strong><br />
Dental exams covered at 90% once per benefit year. Overall plan maximum of $900 per benefit year.<br />
<br />
<strong>Dental Accident:</strong><br />
Reimbursed at 100% to a maximum of $1,000 per accident.<br />
(services must be started within 90 days of accident and completed within 12 months of accident; authorization required)
<br />
<br />
<strong>Travel Insurance:</strong><br />
$5,000,000 of coverage for emergencies and illnesses while travelling.<br />
<br />
<strong>Tutorial:</strong><br />
After 15 days of confinement due to illness or injury.

</p>

</p>
</p>

)
  }
}







class Showint extends React.Component {
 constructor(props) {
    super(props);
    this.state = {
      inverted: true,
tdshide:true
    };
    this.toggleInverted = this.toggleInverted.bind(this);
  }

  toggleInverted(e) {

if (e.target.value === "Ambulance1")
{ 
          this.setState({inverted: false, tdshide:true});


        }
else if (e.target.value === "Practitioners1")
{ 
          this.setState({inverted: true, tdshide: false});
        }

else
{
   this.setState({inverted: true, tdshide: true});
}





  }

