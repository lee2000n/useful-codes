    };

    this.showeinfo= this.showeinfo.bind(this);
 this.showfamily= this.showfamily.bind(this);
 this.showcoverage= this.showcoverage.bind(this);
 this.showintl= this.showintl.bind(this);
 this.showbuy= this.showbuy.bind(this);
 this.showsponsor= this.showsponsor.bind(this);
 this.showcoo= this.showcoo.bind(this);
 this.highlightSearch= this.highlightSearch.bind(this);
  }





