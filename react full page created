class Apps extends React.Component {
  constructor() {
document.querySelector('#mod6286d02d_214e_4434_8718_c0a268984d8b').style.display= "none";
   document.querySelector('#mod068bc57b_c607_4837_88e5_2f587afb495d').style.display= "none";
    super();
    this.state = {
glance :true,
   intel:true,
reshow : true,
pres : true,
dental: true,
personal: true,
exclusion: true,
result:true,
haveresult: true
    };

    this.showshowint = this.showshowint .bind(this);
 this.showshowglance= this.showshowglance.bind(this);
 this.showshowrepates= this.showshowrepates.bind(this);
 this.showprescription= this.showprescription.bind(this);
 this.showdental= this.showdental.bind(this);
 this.showpersonal= this.showpersonal.bind(this);
 this.showexclusion= this.showexclusion.bind(this);
 this.highlightSearch= this.highlightSearch.bind(this);

  }

showshowint () {
    // You can use redux for this.
    this.setState({
glance :true,
   intel:false,
reshow: true,
pres : true,dental: true, personal: true,

exclusion: true
    });
 }



  showshowglance() {
    // You can use redux for this.
    this.setState({
glance :false,
   intel: true,
reshow: true,
pres : true,dental: true, personal: true,
exclusion: true
    });
 
  }

 showshowrepates() {
